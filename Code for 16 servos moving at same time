// C++ code
//
#include <Servo.h>

int position = 0;

int i = 0;

int j = 0;

int k = 0;

int m = 0;

int n = 0;

int o = 0;

int p = 0;

Servo servo_A0;

Servo servo_11;

Servo servo_10;

Servo servo_13;

Servo servo_0;

Servo servo_1;

Servo servo_2;

Servo servo_3;

Servo servo_4;

Servo servo_5;

Servo servo_6;

Servo servo_7;

Servo servo_8;

Servo servo_9;

Servo servo_12;

Servo servo_A1;

void setup()
{
  servo_A0.attach(A0, 500, 2500);
  servo_11.attach(11, 500, 2500);
  servo_10.attach(10, 500, 2500);
  servo_13.attach(13, 500, 2500);
  servo_0.attach(0, 500, 2500);
  servo_1.attach(1, 500, 2500);
  servo_2.attach(2, 500, 2500);
  servo_3.attach(3, 500, 2500);
  servo_4.attach(4, 500, 2500);
  servo_5.attach(5, 500, 2500);
  servo_6.attach(6, 500, 2500);
  servo_7.attach(7, 500, 2500);
  servo_8.attach(8, 500, 2500);
  servo_9.attach(9, 500, 2500);
  servo_12.attach(12, 500, 2500);
  servo_A1.attach(A1, 500, 2500);
}

void loop()
{
  position = 90;
  for (position = 90; position >= 60; position -= 1) {
    servo_A0.write(position);
    servo_11.write(position);
    servo_10.write(position);
    servo_13.write(position);
    servo_0.write(position);
    servo_1.write(position);
    servo_2.write(position);
    servo_3.write(position);
    servo_4.write(position);
    servo_5.write(position);
    servo_6.write(position);
    servo_7.write(position);
    servo_8.write(position);
    servo_9.write(position);
    servo_12.write(position);
    servo_A1.write(position);
    delay(20); // Wait for 20 millisecond(s)
  }
  delay(1000); // Wait for 1000 millisecond(s)
  for (position = 60; position >= 30; position -= 1) {
    servo_A0.write(position);
    servo_11.write(position);
    servo_10.write(position);
    servo_13.write(position);
    servo_0.write(position);
    servo_1.write(position);
    servo_2.write(position);
    servo_3.write(position);
    servo_4.write(position);
    servo_5.write(position);
    servo_6.write(position);
    servo_7.write(position);
    servo_8.write(position);
    servo_9.write(position);
    servo_12.write(position);
    servo_A1.write(position);
    delay(20); // Wait for 20 millisecond(s)
  }
  delay(1000); // Wait for 1000 millisecond(s)
  for (position = 30; position <= 90; position += 1) {
    servo_A0.write(position);
    servo_11.write(position);
    servo_10.write(position);
    servo_13.write(position);
    servo_0.write(position);
    servo_1.write(position);
    servo_2.write(position);
    servo_3.write(position);
    servo_4.write(position);
    servo_5.write(position);
    servo_6.write(position);
    servo_7.write(position);
    servo_8.write(position);
    servo_9.write(position);
    servo_12.write(position);
    servo_A1.write(position);
    delay(20); // Wait for 20 millisecond(s)
  }
  delay(1000); // Wait for 1000 millisecond(s)
  for (position = 90; position <= 120; position += 1) {
    servo_A0.write(position);
    servo_11.write(position);
    servo_10.write(position);
    servo_13.write(position);
    servo_0.write(position);
    servo_1.write(position);
    servo_2.write(position);
    servo_3.write(position);
    servo_4.write(position);
    servo_5.write(position);
    servo_6.write(position);
    servo_7.write(position);
    servo_8.write(position);
    servo_9.write(position);
    servo_12.write(position);
    servo_A1.write(position);
    delay(20); // Wait for 20 millisecond(s)
  }
  delay(1000); // Wait for 1000 millisecond(s)
  for (position = 120; position <= 150; position += 1) {
    servo_A0.write(position);
    servo_11.write(position);
    servo_10.write(position);
    servo_13.write(position);
    servo_0.write(position);
    servo_1.write(position);
    servo_2.write(position);
    servo_3.write(position);
    servo_4.write(position);
    servo_5.write(position);
    servo_6.write(position);
    servo_7.write(position);
    servo_8.write(position);
    servo_9.write(position);
    servo_12.write(position);
    servo_A1.write(position);
    delay(20); // Wait for 20 millisecond(s)
  }
  delay(1000); // Wait for 1000 millisecond(s)
  for (position = 150; position >= 90; position -= 1) {
    servo_A0.write(position);
    servo_11.write(position);
    servo_10.write(position);
    servo_13.write(position);
    servo_0.write(position);
    servo_1.write(position);
    servo_2.write(position);
    servo_3.write(position);
    servo_4.write(position);
    servo_5.write(position);
    servo_6.write(position);
    servo_7.write(position);
    servo_8.write(position);
    servo_9.write(position);
    servo_12.write(position);
    servo_A1.write(position);
    delay(20); // Wait for 20 millisecond(s)
  }
  delay(1000); // Wait for 1000 millisecond(s)
}
